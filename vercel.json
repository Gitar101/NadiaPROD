{
  "version": 2,
  "builds": [
    {
      "src": "batch.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "batch.py"
    }
  ]
}
